{
  "name": "front-end-navigation",
  "version": "1.5.2",
  "description": "Chrome extensions for front-end development.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext js --ext vue ./src ./config --fix && stylelint \"./src/**/*.vue\" --fix",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "analysis": "cross-env NODE_ENV=analysis webpack --progress",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --progress",
    "build:chrome": "rimraf ./chrome-extends/* && cp ./google-extends-manifest.json ./chrome-extends/manifest.json && cross-env NODE_ENV=chrome webpack --progress",
    "test": "cross-env NODE_ENV=chrome BABEL_ENV=test karma start test/unit/karma.conf.js",
    "coverage": "cat test/unit/coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengsixing/front-end-navigation.git"
  },
  "author": "mengsixing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengsixing/front-end-navigation/issues"
  },
  "homepage": "https://github.com/mengsixing/front-end-navigation#readme",
  "dependencies": {
    "element-ui": "^2.13.0",
    "html-webpack-plugin": "^4.0.4",
    "mini-css-extract-plugin": "^0.9.0",
    "rimraf": "^3.0.2",
    "stylelint-webpack-plugin": "^1.2.3",
    "vue": "^2.6.11",
    "vue-loader": "15.9.1",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-merge": "^4.2.2",
    "cssnano": "^4.1.10",
    "autoprefixer": "^9.7.6"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.0",
    "ejs": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.1",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.0",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
